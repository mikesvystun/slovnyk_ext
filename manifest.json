{
  "manifest_version": 2,
  
  "name": "Slovnyk",
  "description": "Suggest better vocabulary",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js", "content.js"]
    }
  ],
  

  "permissions": [
     "activeTab",
     "storage"
  ]

}
